{
  "name": "swc-register",
  "version": "1.0.4",
  "description": "Transpile JSX, TypeScript and esnext features on the fly with swc",
  "main": "register.js",
  "scripts": {
    "build": "swc src -d dist",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "tap --node-arg=--require=./register.js --nyc-arg=--exclude=dist/*.js",
    "prepare": "npx lint-staged"
  },
  "files": [
    "dist",
    "/register.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Songkeys/swc-register.git"
  },
  "keywords": [
    "swc",
    "register",
    "transpiler",
    "typescript"
  ],
  "author": "Songkeys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Songkeys/swc-register/issues"
  },
  "homepage": "https://github.com/Songkeys/swc-register#readme",
  "dependencies": {
    "pirates": "^4.0.5",
    "source-map-support": "^0.5.21",
    "tsconfig-to-swcconfig": "^2.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.38",
    "@types/node": "^18.14.6",
    "@types/source-map-support": "^0.5.6",
    "@types/tap": "^15.0.8",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "tap": "^16.3.4",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@swc/core": ">= 1.3.38"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
