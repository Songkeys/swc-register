{
  "name": "swc-register",
  "version": "1.0.0",
  "description": "Transpile JSX, TypeScript and esnext features on the fly with swc",
  "main": "register.js",
  "scripts": {
    "build": "swc src -d dist",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "tap --node-arg=--require=./register.js --nyc-arg=--exclude=dist/*.js",
    "prepare": "npx lint-staged"
  },
  "files": [
    "dist",
    "/register.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Songkeys/swc-register.git"
  },
  "keywords": [
    "swc",
    "register",
    "transpiler",
    "typescript"
  ],
  "author": "Songkeys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Songkeys/swc-register/issues"
  },
  "homepage": "https://github.com/Songkeys/swc-register#readme",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "joycon": "^3.0.1",
    "jsonc-parser": "^3.0.0",
    "pirates": "^4.0.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.49",
    "@swc/core": "^1.2.78",
    "@swc/helpers": "^0.2.13",
    "@types/node": "^16.6.1",
    "@types/source-map-support": "^0.5.4",
    "@types/tap": "^15.0.5",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "tap": "^15.0.9",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@swc/core": ">= 1.2.58"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
